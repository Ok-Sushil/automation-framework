<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite name="Automation Test Suite" parallel="classes" thread-count="2">

<listeners>
    <listener class-name="com.company.framework.listeners.TestListener"/>
    <listener class-name="com.company.framework.listeners.AnnotationTransformer"/>
</listeners>

<test name="Regression Tests">
    <classes>
        <class name="com.company.framework.tests.LoginTest"/>
        <class name="com.company.framework.tests.LoginTestTwo"/>
    </classes>
</test>



</suite>
